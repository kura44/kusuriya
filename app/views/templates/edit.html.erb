<%= form_for @template do |f| %>
    <div class="field">
        <%= f.label :name %>
        <%= f.text_field :name, :size => 140 %><br>
        <%= f.label :template_id %>
        <%= f.text_field :template_id, :size => 140 %><br>
        <%= f.label :image %>
        <%= f.file_field :image %>
    </div>
    <%= f.submit "編集する" %>
<% end %>